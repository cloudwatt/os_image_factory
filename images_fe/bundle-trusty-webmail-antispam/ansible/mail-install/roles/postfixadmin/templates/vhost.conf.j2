<VirtualHost *:80>
    DocumentRoot "/var/www/postfixadmin"
    ErrorLog "/var/log/apache2/postfixadmin-error_log"
    TransferLog "/var/log/apache2/postfixadmin-access_log"

    <Directory />
        Options +Indexes +FollowSymLinks +ExecCGI
        AllowOverride All
        Order deny,allow
        Allow from all
    Require all granted
    </Directory>
</VirtualHost>